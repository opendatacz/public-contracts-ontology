# --- Vocabulary terms -----------------------------------------

# Druh řízení veřejné zakázky 
# 0..1
pccz:managedAs a rdf:Property ;
  rdfs:domain pc:Contract ;
  rdfs:range pccz:PCManagement ;
  rdfs:label "Druh řízení veřejné zakázky"@cs, "Kind of public contract management"@en ;
  rdfs:comment "Druh řízení veřejné zakázky"@cs, "Kind of public contract management"@en ;
  rdfs:isDefinedBy pccz: .

# Druh řízení
pccz:PCManagement a rdfs:Class ;
  rdfs:label "Druh řízení"@cs, "Kind of management"@en ;
  rdfs:comment "Definuje povolené druhy řízení veřejných zakázek"@cs, 
    "Defines allowed kinds of managament of public contracts"@en ;
  rdfs:isDefinedBy pccz: .

pccz:OpenManagement a pccz:PCManagement ;
  rdfs:label "Otevřené řízení"@cs, "Open managmement"@en ;
  rdfs:isDefinedBy pccz: .

pccz:NarrowManagement a pccz:PCManagement ;
  rdfs:label "Užší řízení"@cs, "Narrow management"@en ;
  rdfs:isDefinedBy pccz: .

pccz:NegotiationWithPublicationManagement a pccz:PCManagement ;
  rdfs:label "Řízení jednací s uveřejněním"@cs, "Negotiation with publication management"@en ;
  rdfs:isDefinedBy pccz: .

pccz:NegotiationWithoutPublicationManagement a pccz:PCManagement ;
  rdfs:label "Řízení jednací bez uveřejnění"@cs, "Negotiation without publication management"@en ;
  rdfs:isDefinedBy pccz: .

pccz:CompetitionDialogueManagement a pccz:PCManagement ;
  rdfs:label "Soutěžní dialog"@cs, "Competition dialogue management"@en ;
  rdfs:isDefinedBy pccz: .

pccz:SimpleUnderLimitManagement a pccz:PCManagement ;
  rdfs:label "Zjednodušené podlimitní řízení"@cs ,
    "Simple under limit management"@en ;
  rdfs:isDefinedBy pccz: .
  
# Limit veřejné zakázky 
# 0..1
pccz:limit a rdf:Property ;
  rdfs:domain pc:Contract ;
  rdfs:range pccz:Limit ;
  rdfs:label "Limit veřejné zakázky"@cs, "Limit of a public contract"@en ;
  rdfs:comment "Limit veřejné zakázky"@cs, "Limit of a public contract"@en ;
  rdfs:isDefinedBy pccz: .

# Limit
pccz:Limit a rdfs:Class ;
  rdfs:label "Limit"@cs, "Limit"@en ;
  rdfs:comment "Definuje povolené limity veřejných zakázek"@cs, 
    "Defines allowed limits of public contracts"@en ;
  rdfs:isDefinedBy pccz: .

pccz:SmallAmount a pccz:Limit ;
  rdfs:label "Zakázka malého rozsahu"@cs, "Public Contract for small amount"@en ;
  rdfs:isDefinedBy pccz: .

pccz:UnderLimit a pccz:Limit ;
  rdfs:label "Podlimitní zakázka"@cs, "Public Contract under the limit"@en ;
  rdfs:isDefinedBy pccz: .

pccz:AboveLimit a pccz:Limit ;
  rdfs:label "Nadlimitní zakázka"@cs, "Public Contract above the limit"@en ;
  rdfs:isDefinedBy pccz: .
  
# Druh veřejné zakázky 
# 0..1
pccz:kind a rdf:Property ;
  rdfs:domain pc:Contract ;
  rdfs:range pccz:Kind ;
  rdfs:label "Druh veřejné zakázky"@cs, "Kind of a public contract"@en ;
  rdfs:comment "Druh veřejné zakázky"@cs, "Kind of a public contract"@en ;
  rdfs:isDefinedBy pccz: .
  
# Druh
pccz:Kind a rdfs:Class ;
  rdfs:label "Druh"@cs, "Druh"@en ;
  rdfs:comment "Definuje povolené druhy veřejných zakázek"@cs,
    "Defines allowed kinds of public contracts"@en ;
  rdfs:isDefinedBy pccz: .

pccz:Supplies a pccz:Kind ;
  rdfs:label "Dodávky"@cs, "Supplies"@en ;
  rdfs:isDefinedBy pccz: .

pccz:Constructions a pccz:Kind ;
  rdfs:label "Stavební práce"@cs, "Constructions"@en ;
  rdfs:isDefinedBy pccz: .

pccz:Services a pccz:Kind ;
  rdfs:label "Služby"@cs, "Services"@en ;
  rdfs:isDefinedBy pccz: .