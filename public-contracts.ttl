# --- Vocabulary terms -----------------------------------------

# Veřejná zakázka
pc:Contract a rdfs:Class ;
  rdfs:label "Veřejná zakázka"@cs, "Public contract"@en ;
  rdfs:comment "Veřejná zakázka"@cs, "Public contract"@en ;
  rdfs:isDefinedBy pc: . 

# Název veřejné zakázky
# 1..1    
dc:title rdfs:domain pc:Contract .

# Předmět veřejné zakázky 
# 1..*
pc:subject a rdf:Property ;
  rdfs:domain pc:Contract ;
  rdfs:range rdfs:Literal ;
  rdfs:label "Předmět"@cs, "Subject"@en ;
  rdfs:comment "Předmět veřejné zakázky"@cs, "Subject of a public contract"@en ;
  rdfs:isDefinedBy pc: .
      
# Datum zveřejnění oznámení o veřejné zakázce
# 1..1    
dc:issued rdfs:domain pc:Contract .

# Datum pro odevzdání nabídek k veřejné zakázce
# 1..1    
pc:bidDeadline a rdf:Property ;
  rdfs:domain pc:Contract ;
  rdfs:range rdfs:Literal ;
  rdfs:subPropertyOf dc:date ;
  rdfs:label "Datum pro odevzdání nabídek"@cs, "Bid deadline"@en ;
  rdfs:comment "Datum pro odevzdání nabídek k veřejné zakázce"@cs,
    "Bid deadline for a public contract"@en ;
  rdfs:isDefinedBy pc: .
  # datatype xsd:date

# Počet podaných nabídek
# 0..1
pc:numberOfBids a rdf:Property ;
  rdfs:label "Počet nabídek"@cs, "Number of bids"@en ;
  rdfs:comment "Počet nabídek podaných k veřejné zakázce"@cs,
    "Number of bids received for a public contract"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range rdfs:Literal ;
  rdfs:isDefinedBy pc: .
  # xsd:int

# Detail nabídky uchazeče o veřejnou zakázku
# 0..*
pc:bid a rdf:Property ;
  rdfs:label "Nabídka"@cs, "Bid"@en ;
  rdfs:comment "Detail nabídky uchazeče o veřejnou zakázku"@cs, "A bid received for a public contract"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range pc:Bid;
  rdfs:isDefinedBy pc: .

# Detail vítězné nabídky
# 0..1
pc:acceptedBid rdfs:subPropertyOf pc:bid ;
  rdfs:label "Vítězná nabídka"@cs, "The accepted bid"@en ;
  rdfs:comment "Detail vítězné nabídky k veřejné zakázce"@cs, "The accepted bid for a public contract"@en .
        
# Abstraktní property pro ceny.
pc:price a rdf:Property ;
  rdfs:label "Cena"@cs, "Price"@en ;
  rdfs:range gr:PriceSpecification ;
  rdfs:isDefinedBy pc: .

# Cena nabídky
pc:bidPrice rdfs:subPropertyOf pc:price ;
  rdfs:label "Cena nabídky"@cs, "Bid price"@en .

# Smluvní cena
# 0..1
pc:contractPrice rdfs:subPropertyOf pc:price ;
  rdfs:label "Smluvní cena"@cs, "Contract price"@en .

# Navýšení k ceně na smlouvě
# 0..1
pc:supplementPrice rdfs:subPropertyOf pc:price ;
  rdfs:label "Dodatková cena"@cs, "Supplement price"@en ;
  rdfs:comment "Dodatková cena navyšující smluvní cenu"@cs,
    "A supplement price increasing the contract price"@en .

# Poskytnutá dotace od třetí strany.
# 0..1
pc:subsidy rdfs:subPropertyOf pc:price ;
  rdfs:label "Dotace"@cs, "Subsidy"@en ;
  rdfs:comment "Dotace k veřejné zakázce"@cs,
    "Subsidy supplemented for a public contract"@en .
        
# Spokojenost s kvalitou odvedené práce dodavatele na zakázce.
# 0..1
pc:quality a rdf:Property ;
  rdfs:label "Kvalita"@cs, "Quality"@en ;
  rdfs:comment "Byla veřejná zakázka dodána v očekávané kvalitě"@cs,
    "Public contract delivered in expected quality"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range rdfs:Literal ;
  rdfs:isDefinedBy pc: .
  # xsd:boolean

# Spokojenost s plněním termínů dodavatelem při realizaci zakázky.
# 0..1
pc:onSchedule a rdf:Property ;
  rdfs:label "Včas"@cs, "On schedule"@en ;
  rdfs:comment "Byla veřejná zakázka splněna včas"@cs, "Public contract delivered on schedule"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range rdfs:Literal ;
  rdfs:isDefinedBy pc: .
  # xsd:boolean
        
# URL, kde lze najít informace k veřejné zakázce
# 0..1
pc:url a rdf:Property ;
  rdfs:label "Odkaz"@cs, "Hyperlink"@en ;
  rdfs:comment "Odkaz na další informace o veřejné zakázce"@cs,
    "A hyperlink to additional information about a public contract"@en ;
  rdfs:subPropertyOf rdfs:seeAlso ;
  rdfs:domain pc:Contract ;
  rdfs:range rdf:Resource ;
  rdfs:isDefinedBy pc: .

# URL doplňujícího dokumentu (smlouva, příloha, ...)
# 0..*
pc:document a rdf:Property ;
  rdfs:label "Dokument"@cs, "Document"@en ;
  rdfs:comment "Doplňující dokument k veřejné zakázce"@cs, "An attached document to a public contract"@en ;
  rdfs:subPropertyOf rdfs:seeAlso ;
  rdfs:domain pc:Contract ;
  rdfs:range dct:Text ;
  rdfs:isDefinedBy pc: .

# Detail platby k veřejné zakázce
# 0..*
pc:payment a rdf:Property ;
  rdfs:label "Platba"@cs, "Payment"@en ;
  rdfs:comment "Jedna platba za veřejnou zakázku"@cs, "One payment for a public contract"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range payment:Payment ;
  rdfs:isDefinedBy pc: .

pc:Bid a rdfs:Class ;
  rdfs:label "Nabídka"@cs, "Bid"@en ;
  rdfs:comment "Nabídka pro veřejnou zakázku"@cs, "A bid for to a public contract"@en ;
  rdfs:isDefinedBy pc: .

pc:bidder a rdf:Property ;
  rdfs:label "Nabízející"@cs, "Bidder"@en ;
  rdfs:comment "Autor nabídky pro veřejnou zakázku"@cs, "A bidder to a public contract"@en ;
  rdfs:domain pc:Bid ;
  rdfs:range gr:BusinessEntity ;
  rdfs:isDefinedBy pc: .

pc:orgNumber a rdf:Property ;
  rdfs:subPropertyOf dc:identifier ;
  rdfs:domain gr:BusinessEntity ;
  rdfs:range rdfs:Literal ;
  rdfs:label "IČ"@cs, "Organization number"@en ;
  rdfs:comment "Identifikační číslo subjektu"@cs, "Identification number of a business entity"@en ;
  rdfs:isDefinedBy pc: .